{
  "name": "@hookkit/core",
  "version": "0.2.0",
  "description": "UseKit 核心包 - 包含所有功能模块",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
