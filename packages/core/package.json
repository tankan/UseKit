  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "usekit",
    "utilities",
    "hooks",
    "composables"
  ],
  "author": "曾星旗",
  "license": "MIT",
  "dependencies": {
    "@hookkit/shared": "workspace:*",
    "@hookkit/http": "workspace:*",
    "@hookkit/date": "workspace:*"
  }
}

