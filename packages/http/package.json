{
  "name": "@hookkit/http",
  "version": "0.2.0",
  "description": "UseKit HTTP 请求模块 - 基于 alova.js 的高级 HTTP 客户端",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "http",
    "request",
    "alova",
    "axios",
    "fetch"
  ],
  "author": "曾星旗",
  "license": "MIT",
  "dependencies": {
    "@hookkit/shared": "workspace:*",
    "alova": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "react": "^18.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  }
}

